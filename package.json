{
  "name": "s3-ui",
  "version": "1.0.0",
  "private": true,
  "description": "Production-ready S3 file management interface with modern UI",
  "keywords": [
    "s3",
    "file-management",
    "nextjs",
    "react",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_OPTIONS=--max-http-header-size=65536 && node server.js",
    "dev:unix": "NODE_OPTIONS=--max-http-header-size=65536 node server.js",
    "dev:next": "next dev --turbopack",
    "build": "next build",
    "start": "set NODE_ENV=production && set NODE_OPTIONS=--max-http-header-size=65536 && node server.js",
    "start:unix": "NODE_ENV=production NODE_OPTIONS=--max-http-header-size=65536 node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"Tests coming soon\" && exit 0",
    "build:analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out node_modules/.cache",
    "postinstall": "npm run type-check",
    "validate": "npm run type-check && npm run lint",
    "prod:build": "npm run validate && npm run build",
    "prod:start": "npm run prod:build && npm run start",
    "docker:build": "docker build -t s3-ui .",
    "docker:run": "docker run -p 3001:3001 s3-ui",
    "health-check": "node -e \"require('http').get('http://localhost:3001/api/health', (res) => process.exit(res.statusCode === 200 ? 0 : 1))\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "@clerk/nextjs": "^6.9.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@supabase/supabase-js": "^2.53.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotted-map": "^2.2.3",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.533.0",
    "next": "^15.1.6",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "svix": "^1.69.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.179.1",
    "three-globe": "^2.44.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.17",
    "eslint": "^9",
    "eslint-config-next": "^15.1.6",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
